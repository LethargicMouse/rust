fn main() {
  let x = "hey";
  let a = x.substr(1, 2);
  putstr(a);
}

fn substr(str, left: u64, right: u64) str {
  str {
    buf: str.buf + left,
    len: right - left
  }
}

fn putiln(i64) {
  puti(i64);
  putstr("\n")
}

fn putstr(str) {
  write(1, str.buf, str.len);
}

fn puti(i64) {
  if (i64 == 0) putstr("0")
  else puti_not_0(i64)
}

fn puti_not_0(i64) {
  if (i64 == 0) return;
  puti_not_0(i64 / 10);
  let c = (48 + i64 % 10) as u8;
  write(1, &c, 1);
}

struct str {
  buf: *u8,
  len: u64
}

extern read: fn(i32, *u8, u64) u64;
extern write: fn(i32, *u8, u64) u64;
